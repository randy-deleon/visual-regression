{
    "extends": [
        "prettier"
    ],
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "rules": {
        "no-unused-vars": "error",
        "no-use-before-define": [2, {
            "functions": false
        }],
        "prefer-const": 1,
        "complexity": [1, 5]
    }
}